<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:Musical1C__frontend.ViewModels"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="Musical1C__frontend.Views.MainContentView"
             x:DataType="viewModels:MainContentViewModel">
    <Grid
        Margin="5"
        ShowGridLines="False"
        ColumnDefinitions="200,*"
        RowDefinitions="Auto,Auto, Auto, *">
        <Button Command="{ Binding NavigateToSearchMusicianViewModelViewModelCommand }" Grid.Column="0" Grid.Row="0" Content="Find musician" Width="110" Margin="1 1"></Button>
        <Button Command="{ Binding NavigateToCreateViewModelCommand }" Grid.Column="0" Grid.Row="1" Content="New Concert" Width="110" Margin="1 1"></Button>
        <Button Command="{ Binding NavigateToGetHistoryViewModelCommand }" Grid.Column="0" Grid.Row="2" Content="Get history" Width="110" Margin="1 1"></Button>
    </Grid>
</UserControl>
